<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

        xmlns:elements="clr-namespace:E_Parser.UI.Elements"
        x:Class="E_Parser.UI.SessionEditor"
        Title="SessionEditor" Height="600" Width="800" DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <TreeView HorizontalAlignment="Left" Height="569" VerticalAlignment="Top" Width="183">
            <Button Content="Button" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        </TreeView>
        
        <ListBox  x:Name="SessionViewer" ItemsSource="{Binding SessionElements}" HorizontalContentAlignment="Center"  HorizontalAlignment="Left" Height="569" Margin="183,0,0,0" VerticalAlignment="Top" Width="364">
            
        </ListBox>

    </Grid>
</Window>
