<elements:ElemBase x:Class="E_Parser.UI.Elements.ElemParameter"
            xmlns:elements="clr-namespace:E_Parser.UI.Elements"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             Height="61" Width="140" MaxHeight="75" MaxWidth="200"
             Session="{Binding CurrentSession, NotifyOnTargetUpdated=True}">
    <Border BorderBrush="LightGray" BorderThickness="2,2,2,2" CornerRadius="10,10,10,10"  
            HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#FF0080BB">
        <Grid>
            <Label HorizontalContentAlignment="Center" Content="Parameter Extension"
                   HorizontalAlignment="Stretch"  VerticalAlignment="Top" 
                   Height="24" Foreground="White" />
            <ComboBox HorizontalAlignment="Stretch" Margin="10,24,10,0" VerticalAlignment="Top" />
        </Grid>
    </Border>
</elements:ElemBase>
